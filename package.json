{
  "name": "log-to-all",
  "version": "0.1.1",
  "description": "Combine a few loggers into one",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm-run-all --parallel develop *:watch",
    "develop": "nodemon ./demo/src",
    "test": "mocha --exit test/**/*.spec.js",
    "test:watch": "mocha test/**/*.spec.js -- --watch",
    "lint": "esw lib",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "eslint-watch": "^7.0.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5"
  },
  "author": "Idan Avrahami",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idanavr/log-to-all"
  },
  "keywords": [
    "logger",
    "log",
    "logging",
    "js",
    "nodejs",
    "console",
    "file",
    "dynamic"
  ]
}